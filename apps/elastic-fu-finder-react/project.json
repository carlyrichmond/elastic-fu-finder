{
  "name": "elastic-fu-finder-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/elastic-fu-finder-react/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/elastic-fu-finder-react"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "fileReplacements": [
            {
              "replace": "apps/elastic-fu-finder-react/src/environments/environment.js",
              "with": "apps/elastic-fu-finder-react/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "elastic-fu-finder-react:build",
        "proxyConfig": "apps/elastic-fu-finder-react/proxy.conf.json"
      },
      "configurations": {
        "development": {
          "buildTarget": "elastic-fu-finder-react:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "elastic-fu-finder-react:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "elastic-fu-finder-react:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "elastic-fu-finder-react:build:development"
        },
        "production": {
          "buildTarget": "elastic-fu-finder-react:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/apps/elastic-fu-finder-react"],
      "options": {
        "coverage": true,
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/elastic-fu-finder-react"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/elastic-fu-finder-react/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
