{
  "name": "elastic-fu-finder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/elastic-fu-finder",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/elastic-fu-finder",
        "outputPath": "dist/apps/elastic-fu-finder"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/elastic-fu-finder"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "elastic-fu-finder:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "elastic-fu-finder:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "elastic-fu-finder:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "elastic-fu-finder:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/elastic-fu-finder/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/elastic-fu-finder/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
